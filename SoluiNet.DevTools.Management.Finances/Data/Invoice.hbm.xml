<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
    namespace="SoluiNet.DevTools.Management.Finances.Data" assembly="SoluiNet.DevTools.Management.Finances">
  <class name="Invoice" table="Invoice">
    <id name="Id">
      <column name="Id" not-null="true"/>
      <generator class="increment" />
    </id>
    <property name="Name">
      <column name="Name" length="50" not-null="true" />
    </property>
    <property name="StorageLocation">
      <column name="StorageLocation" length="255" not-null="true" />
    </property>
  </class>
</hibernate-mapping>