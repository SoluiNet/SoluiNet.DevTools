<UserControl x:Class="SoluiNet.DevTools.Utils.TimeTracking.UI.BrushDefinitionUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SoluiNet.DevTools.Utils.TimeTracking.UI"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <TabControl TabStripPlacement="Bottom">
            <TabItem Header="Simple Gradient">
                <Grid x:Name="SimpleGradientGrid"></Grid>
            </TabItem>
            <TabItem Header="Gradient">
                <Grid x:Name="GradientGrid"></Grid>
            </TabItem>
        </TabControl>
    </Grid>
</UserControl>
